<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">


    <meta name="theme-name" content="quixlab" />

    <title>Quixlab - Bootstrap Admin Dashboard Template by Themefisher.com</title>
    <link rel="icon" type="image/png" sizes="16x16" href="~/images/favicon.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="~/plugins/pg-calendar/css/pignose.calendar.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/plugins/chartist/css/chartist.min.css">
    <link rel="stylesheet" href="~/plugins/chartist-plugin-tooltips/css/chartist-plugin-tooltip.css">
    <link href="~/css/style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/metisMenu.min.css" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/metisMenu/3.0.7/metisMenu.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

    <script>
        $(document).ready(function () {
            console.log("Sidebar script running...");
            $('#menu').metisMenu();
        });
    </script>

</head>


<body>
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>

    

        <div class="nav-header">
            <div class="brand-logo">
                <a asp-action="index" asp-controller="Home">
                    <b class="logo-abbr"><img src="~/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>

        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
@*                         <span class="toggle-icon"><i class="icon-menu"></i></span>
 *@                    </div>
                </div>
                <div class="header-left">
                    <div class="input-group icons">
                        <div class="input-group-prepend">
                        </div>
                        
                    </div>
                </div>
                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown">
                            <a data-toggle="dropdown">
                                <i class="mdi mdi-email-outline"></i>
                                <span class="badge gradient-1 badge-pill badge-primary">3</span>
                            </a>
                            <div class="drop-down animated fadeIn dropdown-menu">
                                <div class="dropdown-content-heading d-flex justify-content-between">
@*                                     <span class="">3 New Messages</span>
 *@
                                </div>
                               
                            </div>
                        </li>
                        <li class="icons dropdown">
                            <a  data-toggle="dropdown">
                                <i class="mdi mdi-bell-outline"></i>
                                <span class="badge badge-pill gradient-2 badge-primary">3</span>
                            </a>
                            <div class="drop-down animated fadeIn dropdown-menu dropdown-notfication">
                                <div class="dropdown-content-heading d-flex justify-content-between">
@*                                     <span class="">2 New Notifications</span>
 *@
                                </div>
                                
                            </div>
                        </li>
                        
                        <li class="icons dropdown">
                            <div class="user-img c-pointer position-relative" data-toggle="dropdown">
                                <span class="activity active"></span>
                                <img src="~/images/user/1.png" height="40" width="40" alt="">
                            </div>
                            <div class="drop-down dropdown-profile   dropdown-menu">
                                <div class="dropdown-content-body">
                                    <ul>
                                        <li>
                                            <a asp-action="Login" asp-controller="User"><i class="icon-user"></i> <span>Profile</span></a>
                                        </li>
                  
                                        <li><a href="page-login.html"><i class="icon-key"></i> <span>Logout</span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    
                    <li>
                        <a asp-controller="Home" asp-action="Index">
                            <i class="icon-speedometer menu-icon"></i><span class="nav-text">Dashboard</span>
                        </a>
                        
                    </li>
                <li class="has-arrow">
                        <a asp-action="CategoryDisplay" asp-controller="Category">
                            <i class="icon-globe-alt menu-icon"></i><span class="nav-text">Category</span>
                        </a>
                        
                    </li>
                   
                    <li>
                        <a class="has-arrow" asp-action="SubCategoryDisplay" asp-controller="SubCategory" aria-expanded="false">
                            <i class="icon-envelope menu-icon"></i> <span class="nav-text">Sub-Category</span>
                        </a>
                       
                    </li>
                    <li>
                        <a class="has-arrow" asp-action="ProductDisplay" asp-controller="Product" aria-expanded="false">
                            <i class="icon-screen-tablet menu-icon"></i><span class="nav-text">Product</span>
                        </a>
                       
                    </li>
                    <li>
                        <a class="has-arrow" asp-action="CustomerDisplay" asp-controller="Customer">
                            <i class="icon-graph menu-icon"></i> <span class="nav-text">Customer</span>
                        </a>
                        
                    </li>
                    
                    <li>
                        <a class="has-arrow" asp-action="UserDisplay" asp-controller="User">
                            <i class="icon-grid menu-icon"></i><span class="nav-text">User</span>
                        </a>
                        
                    </li>
                    <li>
                    <a class ="has-arrow" aria-expanded="false" asp-action="OrderDisplay" asp-controller="Order">
                            <i class="icon-badge menu-icon"></i><span class="nav-text">Order</span>
                        </a>
                    </li>
                    <li>
                        <a class="has-arrow" asp-action="OrderDetailDisplay" asp-controller="OrderDetail">
                            <i class="icon-note menu-icon"></i><span class="nav-text">Order Detail</span>
                        </a>
                        
                    </li>
                    
                    <li>
                        <a class="has-arrow" asp-action="BillDisplay" asp-controller="Bill">
                            <i class="icon-menu menu-icon"></i><span class="nav-text">Bill</span>
                        </a>
                        
                    </li>
                    
                 </ul>
            </div>
        </div>

        @* <div class="footer">
            <div class="copyright">
                <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a> 2018</p>
            </div>
        </div> *@

    

    @RenderBody()

    <div class="footer">
        <div class="copyright">
            <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a> 2018</p>
        </div>
    </div>
    <script src="~/plugins/common/common.min.js"></script>
    <script src="~/js/custom.min.js"></script>
    <script src="~/js/settings.js"></script>
    <script src="~/js/gleek.js"></script>
    <script src="~/js/styleSwitcher.js"></script>
    <script src="~/plugins/chart.js/Chart.bundle.min.js"></script>
    <script src="~/plugins/circle-progress/circle-progress.min.js"></script>
    <script src="~/plugins/d3v3/index.js"></script>
    <script src="~/plugins/topojson/topojson.min.js"></script>
    <script src="~/plugins/datamaps/datamaps.world.min.js"></script>
    <script src="~/plugins/raphael/raphael.min.js"></script>
    <script src="~/plugins/morris/morris.min.js"></script>
    <script src="~/plugins/moment/moment.min.js"></script>
    <script src="~/plugins/pg-calendar/js/pignose.calendar.min.js"></script>
    <script src="~/plugins/chartist/js/chartist.min.js"></script>
    <script src="~/plugins/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js"></script>
    <script src="~/js/dashboard/dashboard-1.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
